<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.leyizhuang.app.foundation.dao.MessageNotificationDAO">


    <select id="queryListByUserIdAndUserType" resultType="cn.com.leyizhuang.app.foundation.pojo.response.MessageNotificationListResponse">
        SELECT id, title, create_time,is_read
        FROM message_notification
        WHERE USER_ID = #{userId} AND identity_type = #{identityType}
        ORDER BY CREATE_TIME DESC
    </select>



</mapper>