<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.leyizhuang.app.foundation.dao.MaCityDAO">

    <select id="findAllCityVO" resultType="CityVO">
        select * from  city order by city_id asc
    </select>

    <select id="findCityVOById" parameterType="java.lang.Long" resultType="CityVO">
        SELECT * FROM city where city_id = #{cityId}
    </select>
</mapper>