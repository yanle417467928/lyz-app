<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.leyizhuang.app.foundation.dao.MaPhysicalClassifyDAO">

  <select id="findPhysicalClassifyList" resultType="PhysicalClassify">
         SELECT
           pc.id,
           pc.physical_classify_name
         FROM
          physical_classify pc
          WHERE
          1=1
  </select>

</mapper>