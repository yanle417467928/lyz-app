<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.leyizhuang.app.foundation.dao.DecorativeCompanyContractDAO">

    <select id="findCompanyContractByCompanyId" resultType="cn.com.leyizhuang.app.foundation.pojo.management.decorativeCompany.DecorativeCompanyContract">
        SELECT
            *
        FROM
            decorative_company_contract
        WHERE
            company_id = #{companyId}
    </select>

</mapper>